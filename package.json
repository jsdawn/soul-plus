    "rollup-plugin-vue": "6.0.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "vite": "^2.5.10",
    "vite-plugin-style-import": "^1.2.1",
    "vuepress": "^2.0.0-beta.26"
