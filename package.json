    "mobile",
    "frontend",
    "component",
    "components",
    "soul"
  ],
  "dependencies": {
    "vue": "^3.2.6",
    "vue-router": "4"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.5",
